nitroNSIP = "192.168.2.54"
nitroUser = "nsroot"
nitroPass = "nsroot"

#Use HTTP or HTTPs to connect to Netscaler
connectiontype = "https"

#Certificate and private key file names on Netscaler	
#Uncomment nspairname, nschainname, nscert, and nskey
#if you don't want to use domain names in files.
#This will disallow multiple main domains.
#nspairname = "le-certificate";
#nschainname = "le-chain";
#nscert = "le-cert.pem";
#nskey = "le-privkey.pem";
nschain = "le-chain.pem";
nscertpath = "/nsconfig/ssl/";

#Responder Policy information
nsrespact = "le-responder-act"
nsresppol = "le-responder-pol"
